<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Amasty_AgeVerification/js/model/display-validator" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_AgeVerification/js/model/display-validator</item>
                                <item name="import" xsi:type="array">
                                    <item name="1" xsi:type="string">Amasty_AgeVerification/js/model/cookie-model</item>
                                </item>
                            </item>
                            <item name="Amasty_AgeVerification/js/model/cookie-model" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_AgeVerification/js/model/cookie-model</item>
                            </item>
                            <item name="Amasty_AgeVerification/js/age-verification-popup" xsi:type="array">
                                <item name="path" xsi:type="string">Amasty_AgeVerification/js/age-verification-popup</item>
                                <item name="import" xsi:type="array">
                                    <item name="1" xsi:type="string">Amasty_AgeVerification/js/model/cookie-model</item>
                                    <item name="2" xsi:type="string">Amasty_AgeVerification/js/model/display-validator</item>
                                </item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
